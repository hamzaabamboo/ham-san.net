{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "pnpm codegen:panda",
    "codegen": "graphql-codegen",
    "codegen:prod": "graphql-codegen -c codegen.prod.yml",
    "codegen:panda": "panda codegen",
    "codegen:park-ui": "pnpx @park-ui/cli add --all",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "compile": "tsc --pretty --noEmit",
    "format": "prettier -c --ignore-path .prettierignore 'src/**/*.{ts,tsx,astro}'",
    "format:fix": "prettier -w --ignore-path .prettierignore 'src/**/*.{ts,tsx,astro}'",
    "fix:all": "pnpm lint:fix && pnpm format:fix"
  },
  "dependencies": {
    "@ark-ui/react": "^5.31.0",
    "@astrojs/check": "^0.9.6",
    "@astrojs/netlify": "^6.6.4",
    "@astrojs/react": "^4.4.2",
    "@types/lodash": "^4.17.23",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.17.3",
    "date-fns": "^4.1.0",
    "eslint-plugin-astro": "^1.6.0",
    "fets": "^0.8.5",
    "front-matter": "^4.0.2",
    "lodash": "^4.17.23",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm": "^3.1.0",
    "micromark-extension-gfm": "^3.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "remark-textr": "^6.1.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@pandabox/prettier-plugin": "^0.1.3",
    "@pandacss/dev": "^1.8.2",
    "@pandacss/eslint-plugin": "^0.3.1",
    "@park-ui/panda-preset": "^0.43.1",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "pandacss-preset-typography": "^0.1.6",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1"
  }
}
